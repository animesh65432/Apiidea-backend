
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Users {
  Id Int @id @default(autoincrement())
  Email String @unique
  projects Projects[]
}
model Projects {
  Id Int @id @default(autoincrement())
  name String
  description String @db.Text
  starterCode String @db.Text
  diagram String @db.Text
  userId Int
  api String
  user Users @relation(fields: [userId],references: [Id])
}